{
  "expo": {
    "name": "Mjekesia Ligjore",
    "slug": "mjekesia-ligjore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-img.png",
      "resizeMode": "contain",
      "backgroundColor": "#29292E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.erindhoxha.mjekesialigjore",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-img.png",
        "backgroundColor": "#29292E"
      },
      "package": "com.erindhoxha.mjekesialigjore"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0b6c9f0a-f1bc-48ce-b408-0906ac9ed7d8"
      }
    },
    "owner": "erindhoxha",
    "plugins": [
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#29292E",
          "image": "./assets/images/splash-img.png",
          "dark": {
            "image": "./assets/images/splash-img.png",
            "backgroundColor": "#29292E"
          },
          "imageWidth": 200
        }
      ]
    ]
  }
}
